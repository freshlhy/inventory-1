<h1>Listing assets</h1>

<table>
  <tr>
    <th>Acquisition date</th>
    <th>Tag</th>
    <th>Tag date</th>
    <th>Serial</th>
    <th>Model</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @assets.each do |asset| %>
  <tr>
    <td><%= asset.acquisition_date %></td>
    <td><%= asset.tag %></td>
    <td><%= asset.tag_date %></td>
    <td><%= asset.serial %></td>
    <td><%= asset.model %></td>
    <td><%= link_to 'Show', asset %></td>
    <td><%= link_to 'Edit', edit_asset_path(asset) %></td>
    <td><%= link_to 'Destroy', asset, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Asset', new_asset_path %>
